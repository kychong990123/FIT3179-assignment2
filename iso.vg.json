{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {"view": {"stroke": ""}},
    "width": 500,
    "height": 50,
    "data": {
      "url": "https://raw.githubusercontent.com/kychong990123/FIT3179-assignment2/main/indian_food.csv"
    },
    "transform": [
      {
        "filter":"datum['region'] == 'East' || datum['region'] == 'West' || datum['region'] == 'North' || datum['region'] == 'South' || datum['region'] == 'North East' "
      },
      {
        "calculate": "{'vegetarian': 'üåø', 'non vegetarian': 'üçñ'}[datum.diet]",
      "as": "emoji"},
      {"window": [{"op": "rank", "as": "rank"}], "groupby": ["region", "diet"]}
    ],
  
    "mark": {"type": "text", "baseline": "middle"},
    "encoding": {
      "x": {"field": "rank", "type": "ordinal", "axis": null},
      "y": {"field": "diet", "type": "nominal", "axis": null, "sort": null},
      "row": {"field": "region", "header": {"title": ""}},
      "text": {"field": "emoji", "type": "nominal"},
      "size": {"value": 20}
    }
  }
  